using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _4
{   
    internal class Program
    {
        
        static void Main(string[] args)
        {
            string stroka = "XIV";
            var Pavel = new Dictionary<char, int>()
            {
                { 'I',1 },
                { 'V',5 },
                { 'X',10 },
                { 'L',50 },
                { 'C',100 },
                { 'D',500 },
                { 'M',1000 },
            };

            int output = 0;
            for (int i = 0; i < stroka.Length; i++)
            {
                if (i == stroka.Length - 1)
                {
                    output += Pavel[stroka[i]];


                }
                else if (Pavel[stroka[i]] < Pavel[stroka[i + 1]])
                {
                    output += Pavel[stroka[i + 1]] - Pavel[stroka[i]];
                    i++;
                }
                else
                {
                    output += Pavel[stroka[i]];
                }
                
            }
            Console.WriteLine(output);
        }
    }
}